{
	"_version": "1.12.0",
	"sap.app": {
		"id": "tdapp",
		"type": "application",
		"applicationVersion": {
			"version": "1.0.0"
		}
	},
	"sap.ui5": {
		"extends": {
			"extensions": {
				"sap.ui.controllerExtenstions":{
					"tdapp.controller.Detail":{
						"controllerNames":[
							"tdapp.controller.extension.AddActionDialog"
						]
					}
				}
			}
		},
		"rootView": {
			"viewName": "tdapp.view.App",
			"type": "XML",
			"async": true,
			"id": "fcl"
		},
		"dependencies": {
			"minUI5Version": "1.60.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.f": {},
				"sap.uxap": {}
			}
		},
		"models":{
			"oActModel": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "./source/Action.json"
			  },
			"oCatModel":{
				"type": "sap.ui.model.json.JSONModel",
				"uri": "./source/ActionCategory.json"
			}
		},
		"config": {
			"fullWidth": true
		},
		"routing": {
			"config": {
				"routerClass": "sap.f.routing.Router",
				"viewType": "XML",
				"viewPath": "tdapp.view",
				"controlId": "flexibleColumnLayout",
				"transition": "slide",
				"bypassed": {
				},
				"async": true
			},
			"routes": [
				{
					"pattern": "page2",
					"name": "page2",
					"target": "page2",
					"layout": "EndColumnFullScreen"
				},
				{
					"pattern": ":layout:",
					"name": "master",
					"target": [
						"master",
						"detail"
					]
				},
				{
					"pattern": "detail/{action}/{layout}",
					"name": "detail",
					"target": [
						"master",
						"detail",
						"detailDetail"
					]
				},
				{
					"pattern": "detailDetail/{action}/{layout}",
					"name": "detailDetail",
					"target": [
						"master",
						"detail",
						"detailDetail"
					]
				}
			],
			"targets": {
				"master": {
					"viewName": "Master",
					"controlAggregation": "beginColumnPages"
				},
				"detail": {
					"viewName": "Detail",
					"controlAggregation": "midColumnPages"
				},
				"detailDetail": {
					"viewName": "DetailDetail",
					"controlAggregation": "endColumnPages"
				}
			}
		}
	}
}